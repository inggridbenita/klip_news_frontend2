{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "camelcase": "off",
        "brace-style": "off",
        "no-undef": "off",
        "no-alert": "off",
        "eqeqeq": "off",
        "no-plusplus": "off"
    }
}
